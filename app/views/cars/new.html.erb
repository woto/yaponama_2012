<% title_new %>

<%= page do |p| %>

  <%= p.breadcrumb do |b| %>
    <%= b.item 'Главная', root_path %>
    <%= b.item 'Личный кабинет' %>
    <%= b.item @meta_title %>
  <% end %>

  <%= p.title do %>
    <%= title_new %>
  <% end %>

  <%= p.content :md do %>
    <%= render "form" %>
  <% end %>

<% end %>
