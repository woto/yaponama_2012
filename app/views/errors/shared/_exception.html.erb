<% if @current_user.seller? %>

  <hr style="margin-top: 200px" />

  <%= row do  %>
    <div class="col-lg-12">
        <ul class="list-unstyled text-sm">
          <% @exception.backtrace.each do |item| %>
            <li><code> <%= item %> </code></li>
          <% end %>
        </ul>
    </div>
  <% end %>
<% end %>
