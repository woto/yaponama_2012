<%= title_index %>

<%= twbs_form_for @grid, :url => smart_route({:prefix => [:filter], :postfix => [jaba2]}, :primary_key => params[:primary_key] ), :remote => true do |f| %>

  <%= panel 'default' do |p| %>

    <%= p.heading do %>
      <%= render 'head' %>
    <% end %>

    <%= p.body do %>
      <%= render :partial => 'grid' %>
    <% end %>

    <%= render 'grid_footer', f: f, p: p %>

  <% end %>

<% end %>
