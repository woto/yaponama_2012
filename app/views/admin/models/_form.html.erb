<%= simple_form_for @resource, :url => polymorphic_path([:admin, @resource], return_path: params[:return_path]) do |f| %>
  <%= row do %>
    <%= c1 do %>
      <%= f.error_notification %>
      <%= f.input :brand_id, as: :hidden %>
      <%= f.input :name %>
      <%= f.input :slang %>
      <%= f.input :from, as: :cars_date %>
      <%= f.input :to, as: :cars_date %>
    <% end %>
  <% end %>
  <%= f.input :content, label: false, input_html: {class: "ckeditor-extended"} %>
  <%= f.buttons %>
<% end %>
