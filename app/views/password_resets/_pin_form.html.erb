<%= row do %>

  <%= c2 do %>

    <%= twbs_form_for @password_reset, url: pin_password_reset_path do |f| %>

      <%= f.alert_danger do %>
        <%= @password_reset.errors[:base].join %>
      <% end %>

      <%= f.twbs_text_field :pin, asterisk: true %>
      <%= f.hidden_field :value %>
      <%= f.hidden_field :with %>

      <%= f.twbs_submit %>

    <% end %>

  <% end %>

<% end %>
