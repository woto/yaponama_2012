<% @meta_title = 'Настройки пользователя' %>

<%= page do |p| %>

  <%= p.breadcrumb do %>
    <%= breadcrumb false do |b| %>
      <% b.item @meta_title %>
    <% end %>
  <% end %>

  <%= p.title do %>
    <%= page_header %>
  <% end %>


  <%= p.content do %>
    <%= render 'form' %>
  <% end %>

<% end %>
