<% @meta_title = "Смена пароля" %>
<% @meta_title_lead = 'Придумайте себе новый пароль' %>

<%= page do |p| %>

  <%= p.breadcrumb do %>
    <%= breadcrumb false do |b| %>
      <%= b.item @meta_title %>
    <% end %>
  <% end %>

  <%= p.title do %>
    <%= page_header %>
  <% end %>

  <%= p.content do %>
    <%= render 'form' %>
  <% end %>

<% end %>
