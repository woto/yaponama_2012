<%= f.nested_fields do %>

  <%= panel 'default' do |p| %>

    <%= p.heading do %>
      <%= p.title do %>
        Вариант доставки   
        <%= f.standard_remove %>
      <% end %>
    <% end %>

    <%= p.body do %>

      <%= f.destroyable do %>

        <%= f.input :name %>
        <%= f.input :active %>
        <%= f.input :sequence %>
        <%= f.input :delivery_cost %>
        <%= f.input :content, class: 'ckeditor-extended' %>

      <% end %>

    <% end %>

  <% end %>

<% end %>
