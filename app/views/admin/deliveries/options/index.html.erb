<% title_index %>

<%= page do |p| %>
  <%= p.breadcrumb do |b| %>
    <%= b.item 'Главная', root_path %>
    <%= b.item 'Администрирование', admin_root_path %>
    <%= b.item @meta_title %>
  <% end %>

  <%= p.title do %>
    <%= page_header %>
  <% end %>

  <%= p.content do %>
    <% @resources.each do |resource| %>
      <%= debug resource.attributes %>
    <% end %>
    <%= link_to 'Новая опция заказа', new_admin_deliveries_option_path %>
  <% end %>

<% end %>
