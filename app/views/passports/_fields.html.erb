<%= nested_fields do %>

  <%= f.destroyable do %>

    <%= f.standard_remove %>

    <%= f.text_input :seriya %>
    <%= f.text_input :nomer %>
    <%= f.text_input :passport_vidan %>
    <%= f.passport_date_input :data_vidachi %>
    <%= f.text_input :kod_podrazdeleniya %>
    <%= f.radio_buttons_themed :gender, Rails.configuration.genders.map {|k,v| [v,k]}.to_a, :last, :first %>
    <%= f.passport_date_input :data_rozhdeniya %>
    <%= f.text_input :mesto_rozhdeniya %>

    <%= f.standard_recreate %>

    <%= render :partial => 'profileables/forms/shared/inputs', :locals => {:f => f} %>

  <% end %>

<% end %>
