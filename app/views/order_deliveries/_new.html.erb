          <select class="form-control" id="order_old_postal_address_id" name="order[old_postal_address_id]" rel="select select-and-fill">
            <option value=""></option>
            <option value="979462528">111111, 1, 1, 1, 1</option>
            <option value="979462527">111111, 1, 1, 1, 1</option>
          </select>

          <input id="order_postal_address_type_old" name="order[postal_address_type]" rel="radio radio-new-old-switcher" type="radio" value="old">

          <input id="return_path" name="return_path" type="hidden" value="/user/orders/563-04-024/delivery/edit?return_path=%2Fuser%2Forders%2F563-04-024%2Fproducts%2Ffilter%3Fprimary_key%3D10994%26return_path%3D%252Fuser%252Forders%252Fstatus%252Fall%253Fprimary_key%253D10993">
          <div class="form-group">
              <input class="btn btn-primary btn-lg top-space bottom-space bottom-space-none btn-warning" name="commit" type="submit" value="Выбрать">&nbsp;&nbsp;&nbsp;или&nbsp;&nbsp;&nbsp;<a class="btn btn-default" data-confirm="Уверены что хотите вернуться назад без сохранения?" href="/user/orders/563-04-024/products/filter?primary_key=10994&amp;return_path=%2Fuser%2Forders%2Fstatus%2Fall%3Fprimary_key%3D10993">Назад</a>
