<%= twbs_form_for @resource, :url => polymorphic_path([:admin, @resource], return_path: params[:return_path]) do |f| %>

  <%= row do %>

    <%= c1 do %>

      <%= f.twbs_select2 :name, rel: 'select2-spare-catalog' %>

      <%= f.twbs_text_area :content, rows: 4 %>

      <%= f.twbs_text_area :intro, class: 'ckeditor-extended' %>

      <%= f.twbs_text_area :page, class: 'ckeditor-extended' %>

      <%= f.twbs_submit %>

    <% end %>

  <% end %>

<% end %>

