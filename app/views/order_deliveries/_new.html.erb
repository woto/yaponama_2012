  <div id="assa-block">

   <%

       resource = OrderDelivery.new

       if @somebody.postal_addresses.any?
         resource.postal_address_type = 'old'
       else
         resource.postal_address_type = 'new'
       end

       resource.new_postal_address = @somebody.postal_addresses.new(postcode: '000000', region: 'Москва', city: 'Москва')

       #TODO Как так? А может это и лучшему :)
       #@resource.old_postal_address = @resource.postal_address
       #@resource.old_postal_address_id = @resource.postal_address_id

   %>

    <%= twbs_form_for resource, url: polymorphic_path([*jaba3, :order_deliveries]), method: :post, remote: true do |f| %>
      <%= render 'payments/postal_addresses', f:f %>
      <%= f.submit 'Расчитать', class: "btn btn-info" %>
    <% end %>
  </div>
</div>
