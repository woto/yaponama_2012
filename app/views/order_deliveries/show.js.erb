<%= render 'aaa' %>

(function() {

  App = typeof exports !== "undefined" && exports !== null ? exports : this;

  $(function() {

    $('#assa-result').show();
    $('#assa-block').hide();

    App.calcRoute('город Москва, Москва, <%= @resource.postal_address.street %> <%= @resource.postal_address.house %>');

    setTimeout(function() {
      $("html, body").animate({scrollTop: $('#auto-calculate').offset().top - 5}, 'fast')
    }, 0)
    

  })

}).call(this);
