<% title_edit %>

<%= page do |p| %>

  <%= p.breadcrumb do |b| %>
    <%= b.item 'Главная', root_path %>
    <%= b.item 'Администрирование', admin_root_path %>
    <%= b.item 'Применимости', admin_spare_infos_path %>
    <%= b.item @resource.to_label, admin_spare_info_path(@resource) %>
    <%= b.item @meta_title %>
  <% end %>

  <%= p.title do %>
    <%= page_header %>
  <% end %>

  <%= p.content :sm do %>
    <%= render :partial => "form" %>
  <% end %>

<% end %>
