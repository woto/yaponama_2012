<% title_index %>

<%= twbs_form_for @grid, :url => smart_route({:prefix => [:filter], :postfix => [jaba2]}, jaba), :remote => true do |f| %>

  <%= panel 'default' do |p| %>

    <%= p.heading do %>
      <%= render 'head' %>
    <% end %>

    <%= p.body do %>
      <%= render 'grid', f: f %>
    <% end %>

  <% end %>


<% end %>

