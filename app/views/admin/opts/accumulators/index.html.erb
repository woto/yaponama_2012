<% title_index %>
<%= page do |p| %>

  <%= p.breadcrumb do %>
    <%= breadcrumb true do |b| %>
      <%= b.item @meta_title %>
    <% end %>
  <% end %>

  <%= p.title do %>
    <%= title_index %>
  <% end %>

  <%= p.content do %>
    <%= twbs_form_for @grid, :url => smart_route({:prefix => [:filter], :postfix => [jaba2]}, jaba), :remote => true do |f| %>

      <%= panel 'default' do |p| %>

        <%= p.heading do %>
          <%= render 'head' %>
        <% end %>

        <%= p.body do %>
          <%= render 'grid', f: f %>
        <% end %>

        <%= render 'grid_footer', f: f, p: p %>

      <% end %>
    <% end %>
  <% end %>
<% end %>
