<%= page do |p| %>

  <%= p.breadcrumb do %>
    <%= breadcrumb false, exception=true do |b| %>
      <%= b.item 'Поиcк запчастей по номеру', 'a' %>
      <%= b.item @meta_title %>
    <% end %>
  <% end %>

  <%= p.title do %>
    <%= page_header %>
  <% end %>

  <%= p.content do %>
    <%= render 'form' %>
    <%= render 'searches/index' %>
  <% end %>

<% end %>

<%= render 'search_footer' %>
