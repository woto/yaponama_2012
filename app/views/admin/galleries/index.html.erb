<% title_index %>

<%= page do |p| %>

  <%= p.breadcrumb do |b| %>
    <%= b.item 'Главная', [:root] %>
    <%= b.item 'Администрирование', [:admin, :root] %>
    <%= b.item @meta_title %>
  <% end %>

  <%= p.title do %>
    <%= page_header %>
  <% end %>

  <%= p.content do %>
    <% @resources.each do |resource| %>
      <%= debug resource.attributes %>
    <% end %>
    <%= link_to 'Новый элемент', new_admin_gallery_path %>
  <% end %>

<% end %>
