<% title_edit %>

<%= page do |p| %>

  <%= p.breadcrumb do |b| %>
    <%= b.item 'Главная', root_path %>
    <%= b.item 'Администрирование', admin_root_path %>
    <%= b.item 'Модели', admin_models_path %>
    <%= b.item @resource.to_label, admin_model_path(@resource) %>
    <%= b.item @meta_title %>
  <% end %>

  <%= p.title do %>
    <%= page_header %>
  <% end %>

  <%= p.content :sm do %>
    <%= render 'form'%>
  <% end %>

<% end %>
