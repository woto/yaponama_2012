<%= icon 'arrow-circle-o-up fa-4x', id: "slide-top" %>

<%= b1 do %>
  <% if Rails.configuration.common['display_counters'] %>
    <%== Rails.configuration.counter['yandex'] %>
    <%== Rails.configuration.counter['mail'] %>
    <%== Rails.configuration.counter['rambler'] %>
    <%== Rails.configuration.counter['google'] %>
    <%== Rails.configuration.counter['openstat'] %>
    <%== Rails.configuration.counter['liveinternet'] %>
  <% end %>
<% end %>

<div class="panel-group hidden" id="accordion">

  <%= panel 'info' do |p| %>
    <%= p.heading do %>
      <%= p.title do %>
        <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#CONFIG">
          CONFIG
        </a>
      <% end %>
    <% end %>
    <%= p.collapse id: 'CONFIG' do %>
      <%= p.body do %>
        <p>
          <b> initial_map_zoom </b>
          <div id="initial_map_zoom"><%= Rails.configuration.map['initial_zoom'] %></div>
        </p>
        <p>
          <b> initial_map_lat </b>
          <div id="initial_map_lat"><%= Rails.configuration.map['initial_lat'] %></div>
        </p>
        <p>
          <b> initial_map_lng </b>
          <div id="initial_map_lng"><%= Rails.configuration.map['initial_lng'] %></div>
        </p>
        <p>
          <b> warehouse_address </b>
          <div id="warehouse_address"><%= Rails.configuration.map['warehouse_address'] %></div>
        </p>
        <p>
          <b> delivery_minute_cost </b>
          <div id="delivery_minute_cost"><%= Rails.configuration.map['delivery_minute_cost'] %></div>
        </p>
        <p>
          <b> google_maps_key </b>
          <div id="google_maps_key"><%= Rails.configuration.map['google_maps_key'] %></div>
        </p>
        <p>
          <b> travel_mode </b>
          <div id="travel_mode"><%= Rails.configuration.map['travel_mode'] %></div>
        </p>
        <p>
          <b> auth-result </b>
          <div id="auth-result"> </div>
        </p>
        <p>
          <b> automatic_calculate_active </b>
          <div id="automatic_calculate_active"><%= Rails.configuration.map['automatic_calculate_active'] %></div>
        </p>
        <p>
          <b> max_automatic_calculated_cost </b>
          <div id="max_automatic_calculated_cost"><%= Rails.configuration.map['max_automatic_calculated_cost'] %></div>
        </p>
      <% end %>
    <% end %>
  <% end %>

  <%= panel 'info' do |p| %>
    <%= p.heading do %>
      <%= p.title do %>
        <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#CatchEnter"> CatchEnter </a>
      <% end %>
    <% end %>
    <%= p.collapse id: "CatchEnter" do %>
      <%= p.body do %>
        <dl>
          <dt> #debug-catch-enter </dt>
          <dd id="debug-catch-enter"> </dd>
        </dl>
      <% end %>
    <% end %>
  <% end %>

  <%= panel 'info' do |p| %>
    <%= p.heading do %>
      <%= p.title do %>
        <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#User">
          User
        </a>
      <% end %>
    <% end %>
    <%= p.collapse id: 'User' do %>
      <%= p.body do %> 
        <dl>
          <dt> #current_user_id </dt>
          <dd id="current_user_id"><%= current_user.try(:id) %></dd>
        </dl>
        <dl>
          <dt> #auth_token </dt>
          <dd id="auth_token"><%= @somebody.try(:auth_token) %></dd>
        </dl>
        <dl>
          <dt> #user_id </dt>
          <dd id="user_id"><%= @somebody.try(:id) %></dd>
        </dl>
        <dl>
          <dt> #current_user_role </dt>
          <dd id="current_user_role"><%= current_user.try(:role) %></dd>
        </dl>
        <dl>
          <dt> #somebody_role </dt>
          <dd id="somebody_role"><%= @somebody.try(:role) %></dd>
        </dl>
        <dl>
          <dt> #admin_zone </dt>
          <dd id="admin_zone" data-value="<%= admin_zone? %>"><%= admin_zone? %></dd>
        </dl>

      <% end %>
    <% end %>
  <% end %>

  <%= panel 'info' do |p| %>
    <%= p.heading do %>
      <%= p.title do %>
        <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#XXX">
          XXX
        </a>
      <% end %>
    <% end %>
    <%= p.collapse id: 'XXX' do %>
      <%= p.body do %>
        XXX
      <% end %>
    <% end %>
  <% end %>

</div>

</body>

</html>
