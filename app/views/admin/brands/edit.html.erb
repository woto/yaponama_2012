<% @meta_title = "Редактирование производителя #{@resource.to_label}" %>

<%= page do |p| %>

  <%= p.breadcrumb do %>
    <%= breadcrumb true do |b| %>
      <%= b.item @meta_title %>
    <% end %>
  <% end %>

  <%= p.title do %>
    <%= page_header %>
  <% end %>

  <%= p.content do %>
    <%= render "form" %>
  <% end %>

<% end %>
