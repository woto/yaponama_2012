<%= twbs_form_for @resource, :url => polymorphic_path([:admin, @resource], return_path: params[:return_path]) do |f| %>

  <%= row do %>

    <%= c1 do %>

      <%= f.twbs_text_field :title %>

      <%= f.twbs_text_field :user_agent %>

      <%= f.twbs_text_field :inet %>

      <%= f.twbs_check_box :block %>

      <%= f.twbs_submit %>

    <% end %>

  <% end %>

<% end %>
