<%= f.nested_fields do %>

  <%= f.destroyable do %>

    <%= panel 'primary', id: 'profile-passport' do |p| %>

      <%= p.heading do %>
        <%= p.title do %>
          Паспорт
          <%= f.standard_remove %>
        <% end %>
      <% end %>

      <%= p.body do %>

        <%= f.twbs_text_field :seriya, asterisk: true %>
        <%= f.twbs_text_field :nomer, asterisk: true %>
        <%= f.twbs_text_field :passport_vidan, asterisk: true %>
        <%= f.twbs_date_select_passport :data_vidachi, asterisk: true %>
        <%= f.twbs_text_field :kod_podrazdeleniya, asterisk: true %>
        <%= f.twbs_collection_radio_buttons :gender, Rails.configuration.genders.map {|k,v| [v,k]}.to_a, :last, :first, variation: :vertical, asterisk: true %>
        <%= f.twbs_date_select_passport :data_rozhdeniya, asterisk: true %>
        <%= f.twbs_text_field :mesto_rozhdeniya, asterisk: true %>

        <%# render :partial => 'profileables/forms/shared/notes', :locals => {:f => f} %>

      <% end %>

    <% end %>

  <% end %>

<% end %>
